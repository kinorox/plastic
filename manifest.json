{
  "manifest_version": 3,
  "name": "Plastic",
  "version": "1.0",
  "description": "Overlay reference images on pixel art websites like r/place",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Plastic"
  }
}